{
    "version": 2,
    "builds": [
      {
        "src": "api/upload.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/upload.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src"
      }
    ]
  }